		// Pagination and filtering variables
		let currentPage = 1;
		let itemsPerPage = 18;
		let filteredDevelopers = [...allDevelopers];

		// Render hot deals (top 4 best value developers)
		function renderHotDeals() {
			const hotDeals = allDevelopers
				.filter(dev => dev.salaryUSD <= 5000 && dev.techLevel >= 75)
				.sort((a, b) => (b.techLevel / b.salaryUSD) - (a.techLevel / a.salaryUSD))
				.slice(0, 4);

			const hotGrid = document.getElementById('hotDealsGrid');
			if (hotGrid) {
				hotGrid.innerHTML = hotDeals.map(dev => `
					<div class="developer-card" style="border: 2px solid #ef4444; background: linear-gradient(135deg, rgba(239, 68, 68, 0.1), rgba(220, 38, 38, 0.05));">
						<div style="background: #ef4444; color: white; padding: 4px 8px; border-radius: 4px; font-size: 10px; font-weight: 700; margin-bottom: 8px; text-align: center;">🔥 HOT DEAL</div>
						<div class="dev-header">
							<div>
								<div class="dev-name">${dev.name}</div>
								<div class="dev-title">${dev.position}</div>
							</div>
							<div class="dev-id">${dev.id}</div>
						</div>
						<div class="dev-tech">
							${dev.technologies.map(tech => `<span class="tech-tag">${tech}</span>`).join('')}
						</div>
						<div class="dev-stats">
							<div class="dev-stat">
								<div class="dev-stat-value">${dev.experienceYears}y</div>
								<div class="dev-stat-label">Experience</div>
							</div>
							<div class="dev-stat">
								<div class="dev-stat-value">$${(dev.salaryUSD/1000).toFixed(1)}K</div>
								<div class="dev-stat-label">Monthly</div>
							</div>
							<div class="dev-stat">
								<div class="dev-stat-value">${dev.rating}★</div>
								<div class="dev-stat-label">Rating</div>
							</div>
							<div class="dev-stat">
								<div class="dev-stat-value">${dev.techLevel}</div>
								<div class="dev-stat-label">Tech Level</div>
							</div>
						</div>
						<div class="dev-details">
							<div class="dev-detail">
								<span class="dev-detail-label">English:</span>
								<span class="dev-detail-value">${dev.englishLevel}</span>
							</div>
							<div class="dev-detail">
								<span class="dev-detail-label">Industry:</span>
								<span class="dev-detail-value">${dev.industry}</span>
							</div>
							<div class="dev-detail">
								<span class="dev-detail-label">Availability:</span>
								<span class="dev-detail-value">${dev.availability}</span>
							</div>
						</div>
						<div class="dev-impact">
							<div class="dev-impact-title">Why "${dev.name}"?</div>
							<div class="dev-impact-text">${dev.businessImpact}</div>
						</div>
						<div class="dev-actions">
							<a href="mailto:hello@just4u.dev?subject=I want ${encodeURIComponent(dev.name)} %23${dev.id}&body=Hi! I'm interested in hiring ${encodeURIComponent(dev.name)} (${dev.id}). Please connect us." class="btn-hire">Hire ${dev.name}</a>
							<a href="mailto:hello@just4u.dev?subject=Tell me more about ${encodeURIComponent(dev.name)} %23${dev.id}" class="btn-contact">Get Details</a>
						</div>
					</div>
				`).join('');
			}
		}

		// Render developers with pagination
		function renderDevelopers() {
			const grid = document.getElementById('developersGrid');
			if (!grid) return;

			const startIndex = (currentPage - 1) * itemsPerPage;
			const endIndex = startIndex + itemsPerPage;
			const developersToShow = filteredDevelopers.slice(startIndex, endIndex);

			grid.innerHTML = developersToShow.map(dev => `
				<div class="developer-card">
					<div class="dev-header">
						<div>
							<div class="dev-name">${dev.name}</div>
							<div class="dev-title">${dev.position}</div>
						</div>
						<div class="dev-id">${dev.id}</div>
					</div>
					<div class="dev-tech">
						${dev.technologies.map(tech => `<span class="tech-tag">${tech}</span>`).join('')}
					</div>
					<div class="dev-stats">
						<div class="dev-stat">
							<div class="dev-stat-value">${dev.experienceYears}y</div>
							<div class="dev-stat-label">Experience</div>
						</div>
						<div class="dev-stat">
							<div class="dev-stat-value">$${(dev.salaryUSD/1000).toFixed(1)}K</div>
							<div class="dev-stat-label">Monthly</div>
						</div>
						<div class="dev-stat">
							<div class="dev-stat-value">${dev.rating}★</div>
							<div class="dev-stat-label">Rating</div>
						</div>
						<div class="dev-stat">
							<div class="dev-stat-value">${dev.techLevel}</div>
							<div class="dev-stat-label">Tech Level</div>
						</div>
					</div>
					<div class="dev-details">
						<div class="dev-detail">
							<span class="dev-detail-label">English:</span>
							<span class="dev-detail-value">${dev.englishLevel}</span>
						</div>
						<div class="dev-detail">
							<span class="dev-detail-label">Industry:</span>
							<span class="dev-detail-value">${dev.industry}</span>
						</div>
						<div class="dev-detail">
							<span class="dev-detail-label">Availability:</span>
							<span class="dev-detail-value">${dev.availability}</span>
						</div>
					</div>
					<div class="dev-impact">
						<div class="dev-impact-title">Why "${dev.name}"?</div>
						<div class="dev-impact-text">${dev.businessImpact}</div>
					</div>
					<div class="dev-actions">
						<a href="mailto:hello@just4u.dev?subject=I want ${encodeURIComponent(dev.name)} %23${dev.id}&body=Hi! I'm interested in hiring ${encodeURIComponent(dev.name)} (${dev.id}). Please connect us." class="btn-hire">Hire ${dev.name}</a>
						<a href="mailto:hello@just4u.dev?subject=Tell me more about ${encodeURIComponent(dev.name)} %23${dev.id}" class="btn-contact">Get Details</a>
					</div>
				</div>
			`).join('');

			// Update pagination
			updatePagination();
		}

		// Update pagination controls
		function updatePagination() {
			const totalPages = Math.ceil(filteredDevelopers.length / itemsPerPage);
			const pagination = document.getElementById('pagination');
			
			if (pagination) {
				pagination.innerHTML = '';
				
				// Previous button
				if (currentPage > 1) {
					pagination.innerHTML += `<button onclick="goToPage(${currentPage - 1})" class="pagination-btn">Previous</button>`;
				}
				
				// Page numbers
				for (let i = 1; i <= Math.min(totalPages, 5); i++) {
					const pageNum = currentPage <= 3 ? i : currentPage - 3 + i;
					if (pageNum <= totalPages) {
						pagination.innerHTML += `<button onclick="goToPage(${pageNum})" class="pagination-btn ${pageNum === currentPage ? 'active' : ''}">${pageNum}</button>`;
					}
				}
				
				// Next button
				if (currentPage < totalPages) {
					pagination.innerHTML += `<button onclick="goToPage(${currentPage + 1})" class="pagination-btn">Next</button>`;
				}
			}
		}

		// Go to specific page
		function goToPage(page) {
			currentPage = page;
			renderDevelopers();
		}

		// Filter by tech stack (quick selection)
		function filterByStack(stack) {
			
			// Clear all filters first
			document.getElementById('salaryMin').value = '1000';
			document.getElementById('salaryMax').value = '10000';
			document.getElementById('expMin').value = '0';
			document.getElementById('expMax').value = '20';
			document.getElementById('techLevel').value = '0';
			document.getElementById('categoryFilter').value = '';
			document.getElementById('industryFilter').value = '';
			document.getElementById('techQuery').value = '';
			
			// Map quick filter names to categories or technologies
			switch(stack) {
				case 'React':
					document.getElementById('categoryFilter').value = 'React';
					break;
				case 'AI':
					document.getElementById('techQuery').value = 'TensorFlow';
					break;
				case 'Node':
					document.getElementById('categoryFilter').value = 'Node.js';
					break;
				case 'Mobile':
					document.getElementById('categoryFilter').value = 'Mobile';
					break;
				case 'DevOps':
					document.getElementById('categoryFilter').value = 'DevOps';
					break;
				case 'Python':
					document.getElementById('categoryFilter').value = 'Python';
					break;
				default:
					document.getElementById('techQuery').value = stack;
			}
			
			applyFilters();
			
			// Scroll to filtered developers with offset to show cards properly
			setTimeout(() => {
				const developersGrid = document.getElementById('developersGrid');
				const yOffset = -100; // Scroll 100px higher to see cards better
				const y = developersGrid.getBoundingClientRect().top + window.pageYOffset + yOffset;
				window.scrollTo({top: y, behavior: 'smooth'});
			}, 100);
		}

		// Apply all filters
		function applyFilters() {
			const salaryMin = parseInt(document.getElementById('salaryMin').value) || 0;
			const salaryMax = parseInt(document.getElementById('salaryMax').value) || 999999;
			const expMin = parseInt(document.getElementById('expMin').value) || 0;
			const expMax = parseInt(document.getElementById('expMax').value) || 999;
			const techLevel = parseInt(document.getElementById('techLevel').value) || 0;
			const category = document.getElementById('categoryFilter').value;
			const industry = document.getElementById('industryFilter').value;
			const techQuery = document.getElementById('techQuery').value.toLowerCase();

			filteredDevelopers = [...allDevelopers];

			// Salary filter
			filteredDevelopers = filteredDevelopers.filter(dev => 
				dev.salaryUSD >= salaryMin && dev.salaryUSD <= salaryMax
			);
			// Experience filter
			filteredDevelopers = filteredDevelopers.filter(dev => 
				dev.experienceYears >= expMin && dev.experienceYears <= expMax
			);

			// Tech Level filter
			if (techLevel > 0) {
				filteredDevelopers = filteredDevelopers.filter(dev => dev.techLevel >= techLevel);
			}

			// Category filter
			if (category) {
				filteredDevelopers = filteredDevelopers.filter(dev => dev.category === category);
			}

			// Industry filter
			if (industry) {
				filteredDevelopers = filteredDevelopers.filter(dev => dev.industry.toLowerCase().includes(industry.toLowerCase()));
			}

			// Tech query filter
			if (techQuery) {
				filteredDevelopers = filteredDevelopers.filter(dev => 
					dev.technologies.some(tech => tech.toLowerCase().includes(techQuery)) ||
					dev.position.toLowerCase().includes(techQuery)
				);
			}

			currentPage = 1;
			
			// Update results count display
			document.getElementById('resultsCount').textContent = filteredDevelopers.length;
			
			renderDevelopers();
		}

		// Clear all filters
		function clearFilters() {
			document.getElementById('salaryMin').value = '1000';
			document.getElementById('salaryMax').value = '10000';
			document.getElementById('expMin').value = '0';
			document.getElementById('expMax').value = '20';
			document.getElementById('techLevel').value = '0';
			document.getElementById('categoryFilter').value = '';
			document.getElementById('industryFilter').value = '';
			document.getElementById('techQuery').value = '';
			
			applyFilters();
		}



		// Initialize on load
		document.addEventListener('DOMContentLoaded', function() {
			console.log('Total developers loaded:', allDevelopers.length);
			console.log('First developer:', allDevelopers[0]);
			
			// Add event listeners to filters
			document.getElementById('categoryFilter').addEventListener('change', applyFilters);
			document.getElementById('techQuery').addEventListener('input', applyFilters);
			document.getElementById('salaryMin').addEventListener('change', applyFilters);
			document.getElementById('salaryMax').addEventListener('change', applyFilters);
			document.getElementById('expMin').addEventListener('change', applyFilters);
			document.getElementById('expMax').addEventListener('change', applyFilters);
			document.getElementById('techLevel').addEventListener('change', applyFilters);
			document.getElementById('industryFilter').addEventListener('change', applyFilters);
			
			console.log('✅ Event listeners added to all filters');
			
			renderHotDeals();
			renderDevelopers();
		});
	</script>
</body>
</html>
